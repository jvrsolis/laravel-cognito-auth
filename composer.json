{
    "name": "kovaloff/laravel-cognito-auth",
    "type": "composer-plugin",
    "description": "An auth provider for Laravel for AWS Cognito.",
    "keywords": [
        "AWS Cognito",
        "Auth Provider"
    ],
    "homepage": "https://github.com/Kovaloff/laravel-cognito-auth",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "composer-plugin-api": "^1.1",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/config": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/auth": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/http": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "aws/aws-sdk-php": "^3.61"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Kovaloff\\LaravelCognitoAuth\\": ""
        }
    },
    "extra": {
        "class": "Kovaloff\\LaravelCognitoAuth\\ComposerPlugin"
    }
}
